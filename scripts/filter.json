{
    "filters": [
        {
            "id": "isDiscordant",
            "isProperPair": "false",
            "isMapped": "true",
            "isMateMapped": "true"
        },
        {
            "id": "hasSoftClip",
            "cigar": "*S*",
            "isMapped": "true",
            "isMateMapped": "true"
        },
        {
            "id": "hasHardClip",
            "cigar": "*H*",
            "isMapped": "true",
            "isMateMapped": "true"
        },
        {
            "id": "isHighQuality",
            "mapQuality": ">=20"
        }
    ],
    "rule": " (hasSoftClip | hasHardClip | isDiscordant) & isHighQuality "
}